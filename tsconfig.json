{
    "compilerOptions": {
        "lib": [
            "es6"
        ],
        "target": "es6",
        "module": "commonjs",
        "outDir": "./dist",
        "baseUrl": "./src",
        "sourceMap": true,
        "allowJs": true,
        "skipLibCheck": true
    },
    "include": [
        "src/**/*"
    ],
    "exclude": [
        "node_modules",
        "dist"
    ]
}
  